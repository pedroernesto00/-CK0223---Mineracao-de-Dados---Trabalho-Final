    "### One Hot Encoder"
   ],
   "metadata": {}
  },
  {
   "cell_type": "code",
   "execution_count": 35,
   "source": [
    "def one_hot_apply(df: pd.core.frame.DataFrame, features:list):\r\n",
    "    encoder = OneHotEncoder(handle_unknown='ignore', sparse=False)\r\n",
    "    \r\n",
    "    for feature in features:\r\n",
    "        reshaped_data = df[feature].values.reshape(len(df[feature]),1)\r\n",
    "        \r\n",
    "        categorys = np.sort(df[feature].unique())\r\n",
    "        categorys = [feature + \"_\" + c for c in categorys]\r\n",
    "        encoded_feature = pd.DataFrame(encoder.fit_transform(reshaped_data), columns=categorys)\r\n",
    "        \r\n",
    "        df.drop(columns=[feature], inplace=True)\r\n",
    "        df = pd.concat([df,encoded_feature], axis=1)\r\n",
    "        \r\n",
    "    return df"
   ],
   "outputs": [],
   "metadata": {}
  },
